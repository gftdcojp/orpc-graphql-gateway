{
  "name": "@gftdcojp/orpc-graphql-gateway",
  "version": "0.1.0",
  "description": "oRPC routerからGraphQL schemaを自動生成するライブラリ",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples/README.md",
    "examples/nextjs-integration.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gftdcojp/orpc-graphql.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:examples": "vitest run examples/",
    "example": "tsx examples/run-example.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "keywords": [
    "orpc",
    "graphql",
    "schema",
    "zod",
    "rpc"
  ],
  "author": "gftdcojp",
  "license": "MIT",
  "dependencies": {
    "graphql": "^16.9.0",
    "zod": "^4.1.12"
  },
  "pnpm": {
    "overrides": {
      "graphql": "^16.9.0"
    }
  },
  "devDependencies": {
    "@apollo/client": "^3.14.0",
    "@apollo/server": "^4.10.3",
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^22.7.5",
    "@vitest/coverage-v8": "^4.0.6",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vitest": "^4.0.6"
  },
  "peerDependencies": {
    "zod": "^4.1.12"
  }
}
